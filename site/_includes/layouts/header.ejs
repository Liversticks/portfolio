<header>
    <nav class="navbar navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <span id="siteTitle" class="navbar-brand"><a href="/"><%= siteTitle %></a></span>
                <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#myHeader" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse" id="myHeader">
                <ul class="nav navbar-nav">
                    <% menuLinks.forEach(function(link) { %>
                        <li><a href="<%= link.link %>"><%= link.name %></a></li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </nav>
</header>